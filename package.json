{
  "name": "@ensofinance/v2-contracts",
  "version": "0.2.8",
  "devDependencies": {
    "@ensofinance/weiroll": "^1.1.0",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@0.3.0-beta.12",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.5.0",
    "@rari-capital/solmate": "^6.3.0",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@weiroll/weiroll.js": "^0.3.0",
    "chai": "^4.3.6",
    "chai-ethers": "^0.0.1",
    "clones-with-immutable-args": "^1.0.0",
    "cross-env": "^7.0.2",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.7",
    "fs-extra": "^10.1.0",
    "hardhat": "^2.9.6",
    "hardhat-deploy": "^0.11.10",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-packager": "^1.4.1",
    "mocha": "^9.2.2",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.21",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.8.0",
    "typechain": "^8.0.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@typechain/ethers-v5": "^10.0.0",
    "typechain": "^8.0.0"
  },
  "files": [
    "/contracts",
    "/artifacts",
    "/typechain",
    "/dist/**/*.d.ts",
    "/dist/**/*.d.ts.map",
    "/dist/**/*.js",
    "/dist/**/*.js.map"
  ],
  "scripts": {
    "prepare": "hardhat typechain",
    "lint": "eslint \"**/*.{js,ts}\" && solhint src/**/*.sol",
    "lint:fix": "eslint --fix \"**/*.{js,ts}\" && solhint --fix src/**/*.sol",
    "format": "prettier --check \"**/*.{ts,js,sol}\"",
    "format:fix": "prettier --write \"**/*.{ts,js,sol}\"",
    "compile": "hardhat compile",
    "void:deploy": "hardhat deploy --report-gas --export-all deployments/deployments.json",
    "test": "cross-env HARDHAT_DEPLOY_FIXTURE=true HARDHAT_COMPILE=true mocha --bail --recursive test",
    "gas": "cross-env REPORT_GAS=true hardhat test",
    "coverage": "cross-env HARDHAT_DEPLOY_FIXTURE=true hardhat coverage",
    "dev:node": "cross-env MINING_INTERVAL=\"3000,5000\" hardhat node --hostname 0.0.0.0",
    "dev": "cross-env MINING_INTERVAL=\"3000,5000\" hardhat node --hostname 0.0.0.0 --watch",
    "local:dev": "hardhat --network localhost deploy --watch",
    "execute": "node ./scripts/_scripts.js run",
    "deploy": "node ./scripts/_scripts.js deploy",
    "verify": "node ./scripts/_scripts.js verify",
    "export": "node ./scripts/_scripts.js export",
    "export-all": "node ./scripts/_scripts.js export-all deployments.json",
    "hardhat": "hardhat",
    "fork:execute": "node ./scripts/_scripts.js fork:run",
    "fork:deploy": "node ./scripts/_scripts.js fork:deploy",
    "fork:dev": "node ./scripts/_scripts.js fork:dev",
    "fork:node": "node ./scripts/_scripts.js fork:node",
    "fork:test": "node ./scripts/_scripts.js fork:test",
    "package:publish": "hardhat prepare-package && npm publish"
  }
}
